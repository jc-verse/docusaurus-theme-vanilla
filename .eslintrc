{
  "root": true,
  "extends": ["jc"],
  "rules": {
    "react/no-array-index-key": "off",

    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "args": "after-used",
        "ignoreRestSiblings": true,
        "vars": "all"
      }
    ],

    "import/no-unresolved": [
      "error",
      { "ignore": ["^@docusaurus/", "^@theme/"] }
    ]
  }
}
